# Makefile for partition experiments

all: n_1000_m_20000 n_10000_m_2000 n_50000_m_400

n_1000_m_20000:
	export npart=1000 && \
	export mrows=20000 && \
	./runner.sh

n_10000_m_2000:
	export npart=10000 && \
	export mrows=2000 && \
	./runner.sh

n_50000_m_400:
	export npart=50000 && \
	export mrows=400 && \
	./runner.sh

